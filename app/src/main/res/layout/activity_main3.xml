<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity3">

    <ImageView
        android:id="@+id/header"
        android:layout_width="410dp"
        android:layout_height="49dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/header" />

    <TextView
        android:id="@+id/consejo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="171dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="171dp"
        android:text="PRINCIPAL"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header" />

    <ImageButton
        android:id="@+id/categoria"
        android:layout_width="321dp"
        android:layout_height="102dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="45dp"
        android:contentDescription="categorias"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header"
        app:srcCompat="@drawable/categoria" />

    <ImageView
        android:id="@+id/imageCategoria"
        android:layout_width="86dp"
        android:layout_height="84dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/categoria"
        app:layout_constraintEnd_toEndOf="@+id/categoria"
        app:layout_constraintTop_toTopOf="@+id/categoria"
        app:srcCompat="@drawable/iconcategoria" />

    <ImageButton
        android:id="@+id/estadistica"
        android:layout_width="321dp"
        android:layout_height="102dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="45dp"
        android:contentDescription="estadisticas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/categoria"
        app:srcCompat="@drawable/estadistica" />

    <ImageView
        android:id="@+id/imageEstadistica"
        android:layout_width="86dp"
        android:layout_height="84dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/estadistica"
        app:layout_constraintEnd_toEndOf="@+id/estadistica"
        app:layout_constraintTop_toTopOf="@+id/estadistica"
        app:srcCompat="@drawable/iconestadistica" />

    <ImageButton
        android:id="@+id/consejos"
        android:layout_width="321dp"
        android:layout_height="102dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="45dp"
        android:contentDescription="@string/consejos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/estadistica"
        app:srcCompat="@drawable/consejos" />

    <ImageView
        android:id="@+id/imageConsejos"
        android:layout_width="86dp"
        android:layout_height="84dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/consejos"
        app:layout_constraintEnd_toEndOf="@+id/consejos"
        app:layout_constraintTop_toTopOf="@+id/consejos"
        app:srcCompat="@drawable/imgconsejos" />

    <ImageButton
        android:id="@+id/registro"
        android:layout_width="321dp"
        android:layout_height="102dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="45dp"
        android:contentDescription="registros"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/consejos"
        app:srcCompat="@drawable/registro" />

    <ImageView
        android:id="@+id/imageRegistro"
        android:layout_width="86dp"
        android:layout_height="84dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/registro"
        app:layout_constraintEnd_toEndOf="@+id/registro"
        app:layout_constraintTop_toTopOf="@+id/registro"
        app:srcCompat="@drawable/imgregistro" />

    <ImageButton
        android:id="@+id/cerrarSesion"
        android:layout_width="56dp"
        android:layout_height="57dp"
        android:layout_marginBottom="17dp"
        android:contentDescription="salir"
        app:layout_constraintBottom_toTopOf="@+id/footer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/cerrarsesion" />

    <ImageView
        android:id="@+id/footer"
        android:layout_width="409dp"
        android:layout_height="54dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/footer" />

</androidx.constraintlayout.widget.ConstraintLayout>